#ifndef INCLUDE_TEXTURES_H_
#define INCLUDE_TEXTURES_H_

typedef struct {
  unsigned int 	 width;
  unsigned int 	 height;
  unsigned int 	 bytes_per_pixel;
  unsigned char	 pixel_data[];
} texture_t;

#endif /* INCLUDE_TEXTURES_H_ */
